{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "List All Companies",
      "type": "python",
      "request": "launch",
      "program": "run_cmd.py",
      "args": "--list companies",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": "-t VALE3",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": { "PYTHONPATH": "${workspaceRoot}" }
    }
  ]
}
